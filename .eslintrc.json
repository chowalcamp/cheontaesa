{
    "extends": "next/core-web-vitals",
    "rules": {
      // 여기에 추가적인 규칙을 설정할 수 있습니다.
      "react/react-in-jsx-scope": "off", // Next.js에서는 React를 자동으로 가져옵니다.
      "no-console": "warn", // console.log 사용 시 경고
      "no-multi-spaces": "off", // 여러 공백 허용
      "space-in-parens": "off", // 괄호 안의 공백 허용
      "keyword-spacing": "off", // 키워드 앞뒤의 공백 허용
      "array-bracket-spacing": "off", // 배열 괄호 안의 공백 허용
      "object-curly-spacing": "off" // 객체 중괄호 안의 공백 허용
    },
    "env": {
    "browser": true,
    "es2021": true
    },
    "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
    }
  }